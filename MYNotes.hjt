ALTER PROCEDURE G_IsGatewayDoc
	@countryId int,
	@Lit_Doc_Name nvarchar(250) = null,
	@Lit_Doc_ID int = null
AS
BEGIN
	DECLARE @IsGatewayDoc nvarchar(200)
	DECLARE @ISEMEA int = null
	DECLARE @ISWHITEPAPER int = 0
	DECLARE @code varchar(5)
	--STEP1 We Check Is AMEA
	--A: Create table get countries
	CREATE TABLE #countrylist (countryid int, countryName nvarchar(500))  
    INSERT INTO #countrylist exec [pq_product].[dbo].[g_regionProd_countryList] -100, null, 231  
    SET @ISEMEA = (Select COUNT(*) from #countrylist where countryId = @countryId)  
    DROP TABLE #countrylist  
	--STEP 2 DETERMINE IF THIS IS A WHITEPAPER
	--
	CREATE TABLE #RESULTSET (RESULTITEM int)
	IF @Lit_Doc_Name is NULL
		BEGIN
		 INSERT INTO #RESULTSET EXEC G_LitDocsIsWhitePaperByDocumentID @Lit_Doc_ID
		 SET @code = 'BYID'
		END
	ELSE
	    BEGIN
			INSERT INTO #RESULTSET EXEC G_LitDocsIsWhitePaper @Lit_Doc_Name
			SET @code = 'BYDC'
	    END 
		
	SET @ISWHITEPAPER = (SELECT [RESULTITEM] FROM #RESULTSET)
	DROP TABLE #RESULTSET
	--STEP 3 DETERMINE IF BOTH ARE 1
	If @ISEMEA = 1 AND @ISWHITEPAPER = 1 
	   BEGIN 
		SET @IsGatewayDoc = '&ISG=1'
	   END	
	Else
		SET @IsGatewayDoc = ''
		
		
		
		SELECT @IsGatewayDoc AS IsGatewayDoc
END
GO


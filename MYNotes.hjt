using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Net;
using System.Net.Mail;


namespace MIEMEAProductRegistrationFTPImport
{
    class gmMailSender
    {
        public static bool gmailEmailSend( string fromEmailAddress, string toEmailMailAddress,
            string mailSubject, List <string> messageparts,
            bool isHtml, string networkCredentialUserName, string networkCredentialPassWord)
        {
            try
            {
                MailMessage mail = new MailMessage();
                SmtpClient SmtpServer = new SmtpClient("smtp.gmail.com" );
                mail.From = new MailAddress(fromEmailAddress);
                mail.To.Add(toEmailMailAddress);
                mail.Subject = mailSubject;
                mail.Body = buildBodyFromMessageParts(messageparts);
                mail.IsBodyHtml = (isHtml) ? true : false ;
                SmtpServer.Port = 587;
                SmtpServer.Credentials = new System.Net.NetworkCredential(networkCredentialUserName, networkCredentialPassWord);
                SmtpServer.EnableSsl = true;
                SmtpServer.Send(mail);
                return true ;
            }
            catch (Exception )
            {
                return false ;

            }
        }


        private static string buildBodyFromMessageParts( List<string > messageparts)
        {
            StringBuilder oStringBuilder = new StringBuilder();
            foreach (string messagepart in messageparts)
            {
                oStringBuilder.Append(messagepart + @"<br />");
            }
            return oStringBuilder.ToString();
        }
    }
}

